        <terms:contributor rdf:resource="http://www.garcia-castro.com/foaf.rdf#me"/>
        <owl:imports rdf:resource="http://www.w3.org/2006/time"/>
        <terms:publisher rdf:resource="https://www.tno.nl"/>
        <terms:creator rdf:resource="https://www.linkedin.com/in/lauradaniele"/>
    </owl:Ontology>
    


